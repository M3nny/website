{
  "name": "m3nny_website",
  "type": "module",
  "version": "0.0.1",
  "license": "WTFPL",
  "repository": "M3nny/website",
  "homepage": "https://github.com/M3nny/website",
  "bugs": "https://github.com/M3nny/website/issues",
  "author": "M3nny",
  "scripts": {
    "clean": "bun urara.js clean",
    "tsc": "tsc -p tsconfig.node.json",
    "tsc:watch": "tsc -w -p tsconfig.node.json",
    "urara:build": "bun urara.js build",
    "urara:watch": "bun urara.js watch",
    "kit:dev": "cross-env NODE_OPTIONS=--max_old_space_size=7680 bunx --bun vite dev",
    "kit:build": "cross-env NODE_OPTIONS=--max_old_space_size=7680 vite build",
    "dev:parallel": "npm-run-all -p -r tsc:watch urara:watch \"kit:dev {@} \" --",
    "dev": "npm-run-all -s tsc \"dev:parallel {@} \" --",
    "build": "npm-run-all -s tsc urara:build kit:build clean",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@iconify-json/heroicons-outline": "^1.1.10",
    "@iconify-json/heroicons-solid": "^1.1.11",
    "@iconify-json/simple-icons": "^1.1.90",
    "@sveltejs/adapter-netlify": "^2.0.8",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/adapter-vercel": "^2.4.3",
    "@sveltejs/kit": "^1.30.3",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.11.16",
    "@types/unist": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@unocss/extractor-svelte": "^0.52.7",
    "@vite-pwa/sveltekit": "^0.1.3",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "daisyui": "^2.52.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "fenceparser": "^2.2.0",
    "fff-flavored-frontmatter": "1.0.0-alpha.1",
    "github-slugger": "^2.0.0",
    "mdast-util-to-string": "^3.2.0",
    "mdsvex": "^0.10.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.33",
    "postcss-lightningcss": "^0.7.0",
    "prettier-plugin-svelte": "^2.10.1",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-external-links": "^2.1.0",
    "rehype-slug": "^5.1.0",
    "remark": "^14.0.3",
    "remark-fff": "1.0.0-alpha.1",
    "remark-footnotes": "~2.0.0",
    "shiki-twoslash": "^3.1.2",
    "svelte": "^3.59.2",
    "svelte-check": "^3.6.3",
    "svelte-preprocess": "^5.1.3",
    "sveltekit-embed": "^0.0.13",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5",
    "unist-util-visit": "^4.1.2",
    "unocss": "^0.52.7",
    "vite": "^4.5.2",
    "vite-imagetools": "^4.0.19",
    "vite-plugin-pwa": "^0.14.7",
    "workbox-build": "^6.6.0",
    "workbox-window": "^6.6.0"
  },
  "overrides": {
    "vite>rollup": "^3.10.0"
  }
}
